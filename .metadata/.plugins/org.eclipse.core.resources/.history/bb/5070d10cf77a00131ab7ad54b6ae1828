package com.gd.tetris.model;

public class GameObject {
	
	private int x;
	private int y;
	
	public void down() {
		y--;
		if (y < 0) {
			y = 0;
		}
	}
	
	public void left() {
		x--;
		if (x < 0) {
			x = 0;
		}
	}
	
	public void right() {
		x++;
	}
	
	public int getX() {
		return x;
	}
	public void setX(int x) {
		this.x = x;
	}
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y = y;
	}
}
