package com.gd.tetris.model;

import java.util.ArrayList;
import java.util.List;

public class Model {
	
	int[][] tiles ;
	ArrayList<GameObject> objs = new ArrayList<GameObject>();
	int rows;
	int cols;
	
	public Model(int rows, int cols) {
		this.rows = rows;
		this.cols = cols;
		tiles = new int[rows][cols];
	}
}
